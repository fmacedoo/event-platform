{
    "root": true,
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/strict",
        "plugin:prettier/recommended",
        "prettier/react",
        "react-app"
    ],
    "parser": "babel-eslint",
    "rules": {
        "jsx-a11y/anchor-is-valid": 0,
        "no-invalid-this": 0,
        "callback-return": 0
    }
}
